public class usingVariables{
    public static void main(String [] args){
        int roomNumber = "113";
        double valueCloseTo = "2.71828";
        String learningAbout = "Computer Science";
        System.out.println("This is room # " + roomNumber);
        System.out.println("e is close to " + valueCloseTo);
        System.out.println("I am learning a bit about " + learningAbout);
    }
 }
