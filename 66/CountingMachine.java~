import java.util.Scanner;
public class CountingMachine{
    public static void main(String[] args){
        Scanner sc = new Scanner(System.in);
        System.out.print("Count to: ");
        int x = sc.nextInt();
        for(int y = 0; y <= x; y += 1){
            System.out.print(y);
            }
        }
    } 
        
        
